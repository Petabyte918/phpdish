<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd" xmlns:gedmo="http://gediminasm.org/schemas/orm/doctrine-extensions-mapping">

  <entity name="PHPDish\Bundle\CoreBundle\Model\Notification" table="notifications">
    <many-to-one field="topic" target-entity="PHPDish\Bundle\ForumBundle\Model\TopicInterface" fetch="LAZY">
      <join-columns>
        <join-column name="topic_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="reply" target-entity="PHPDish\Bundle\ForumBundle\Model\ReplyInterface" fetch="LAZY">
      <join-columns>
        <join-column name="reply_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="post" target-entity="PHPDish\Bundle\PostBundle\Model\PostInterface" fetch="LAZY">
      <join-columns>
        <join-column name="post_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="comment" target-entity="PHPDish\Bundle\PostBundle\Model\CommentInterface" fetch="LAZY">
      <join-columns>
        <join-column name="comment_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="category" target-entity="PHPDish\Bundle\PostBundle\Model\CategoryInterface" fetch="LAZY">
      <join-columns>
        <join-column name="category_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="payment" target-entity="PHPDish\Bundle\PaymentBundle\Model\PaymentInterface" fetch="LAZY">
      <join-columns>
        <join-column name="payment_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>

</doctrine-mapping>
