<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PHPDish\Bundle\CoreBundle\Model\MessageThread" table="message_threads">
    <many-to-one field="createdBy" target-entity="PHPDish\Bundle\UserBundle\Model\User" fetch="LAZY">
      <join-columns>
        <join-column name="created_by_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
